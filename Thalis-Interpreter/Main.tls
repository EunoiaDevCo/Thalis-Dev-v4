Import IO;

class Application
{
	public virtual void Func()
	{
		IO.Println("Application::Func()");
	}

	uint32 v1;
};

class Game -> inherit[Application]
{
	public virtual void Func()
	{
		IO.Println("Game::Func()");
	}

	uint32 v2;
};

class Main
{
	public static void Main()
	{
		Application* app = new Game();
		app->v1 = 10;
		app->v2 = 20;

		app->Func();

		delete app;
	}
};